<%- include('../templeteAdmin/sidebar') %>
<body>

    <div class="content">
        
        <h1>Categori Traning</h1>
        <a href='/tranings/formcreate' class="btn btn-primary">Create</a>
        <br>
        <table class="data-table">
            <thead>
                <tr>
                    <th>No</th>
                    <th>Name</th>
                    <th>Action</th>
                </tr>
            </thead>
            <tbody>
                    <% traning.forEach((traning, index) => { %>
                    <tr>
                    <th scope="row"><%= index + 1 %></th>
                    <td><%= traning.name_category_traning %></td>

                    <td>
                        <button type="button" class="btn btn-primary" href="">Edit</button>
                        <a class="btn btn-danger" href="" onclick="return confirm('Are you sure you want to delete this product?')">Delete</a>
                    </td>
                  </tr>
                <% }); %>
                </tbody>
        </table>
    </div>
</body>

